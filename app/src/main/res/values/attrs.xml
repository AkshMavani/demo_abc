<resources>

    <declare-styleable name="WheelPicker">
        <attr name="scroll_data" format="reference" />
        <attr name="scroll_background_color" format="color" />
        <attr name="scroll_selected_item_background" format="color" />
        <attr name="scroll_selected_item_position" format="integer" />
        <attr name="scroll_item_text_size" format="dimension" />
        <attr name="scroll_item_text_color" format="color" />
        <attr name="scroll_selected_item_text_color" format="color" />
        <attr name="scroll_maximum_width_text" format="string" />
        <attr name="scroll_maximum_width_text_position" format="integer" />
        <attr name="scroll_visible_item_count" format="integer" />
        <attr name="scroll_item_space" format="dimension" />
        <attr name="scroll_indicator" format="boolean" />
        <attr name="scroll_indicator_color" format="color" />
        <attr name="scroll_indicator_size" format="dimension" />
        <attr name="scroll_atmospheric" format="boolean" />
        <attr name="scroll_item_align" format="enum">
            <enum name="center" value="0" />
            <enum name="left" value="1" />
            <enum name="right" value="2" />
        </attr>
    </declare-styleable>
    <attr name="background_color_main" format="color">
    </attr>
    <declare-styleable name="DragLayout_Layout">
        <attr name="layout_slideable" format="boolean" />
        <attr name="layout_peekHeight" format="dimension" />
        <attr name="layout_anchorPoint" format="reference" />
    </declare-styleable>
