<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="@dimen/_52sdp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:orientation="vertical"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:orientation="horizontal"
            android:layout_marginHorizontal="@dimen/_4sdp"
            android:layout_height="@dimen/_100sdp">
               <androidx.cardview.widget.CardView
                   android:layout_width="@dimen/_150sdp"
                   app:cardBackgroundColor="@color/black"
                   android:id="@+id/caed_language_first"

                   android:layout_marginRight="@dimen/_4sdp"
                    app:cardCornerRadius="@dimen/_10sdp"
                   android:layout_height="match_parent">
                   <LinearLayout
                       android:layout_width="match_parent"
                       android:orientation="vertical"
                       android:layout_margin="@dimen/_8sdp"
                       android:layout_height="match_parent">
                        <ImageView
                            android:layout_width="match_parent"
                            android:layout_weight="1"
                            android:padding="@dimen/_12sdp"
                            android:src="@drawable/a14"
                            android:layout_height="wrap_content"/>
                       <TextView
                           android:layout_width="match_parent"
                           android:text="hwllo"
                           android:textSize="@dimen/_14ssp"
                           android:gravity="center"
                           android:textColor="@color/blue"
                           android:layout_height="wrap_content"/>
                   </LinearLayout>
               </androidx.cardview.widget.CardView>
            <androidx.cardview.widget.CardView
                android:layout_width="@dimen/_150sdp"
                   app:cardBackgroundColor="@color/Red"
                android:id="@+id/caed_language_second"
                android:layout_marginStart="@dimen/_4sdp"
                app:cardCornerRadius="@dimen/_10sdp"
                android:layout_height="match_parent">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:orientation="vertical"
                    android:layout_margin="@dimen/_8sdp"
                    android:layout_height="match_parent">
                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_weight="1"
                        android:padding="@dimen/_12sdp"
                        android:src="@drawable/a4"
                        android:layout_height="wrap_content"/>
                    <TextView
                        android:layout_width="match_parent"
                        android:text="hwllo"
                        android:textSize="@dimen/_14ssp"
                        android:gravity="center"
                        android:textColor="@color/blue"
                        android:layout_height="wrap_content"/>
                </LinearLayout>
            </androidx.cardview.widget.CardView>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:orientation="horizontal"
            android:layout_marginTop="@dimen/_16sdp"
            android:layout_marginHorizontal="@dimen/_4sdp"
            android:layout_height="@dimen/_100sdp">
               <androidx.cardview.widget.CardView
                   android:layout_width="@dimen/_150sdp"
                   app:cardBackgroundColor="@color/Alien_Green"
                   android:id="@+id/caed_language_third"
                   android:layout_marginRight="@dimen/_4sdp"
                    app:cardCornerRadius="@dimen/_10sdp"
                   android:layout_height="match_parent">
                   <LinearLayout
                       android:layout_width="match_parent"
                       android:orientation="vertical"
                       android:layout_margin="@dimen/_8sdp"
                       android:layout_height="match_parent">
                        <ImageView
                            android:layout_width="match_parent"
                            android:layout_weight="1"
                            android:src="@drawable/a9"
                            android:padding="@dimen/_12sdp"
                            android:layout_height="wrap_content"/>
                       <TextView
                           android:layout_width="match_parent"
                           android:text="hwllo"
                           android:textSize="@dimen/_14ssp"
                           android:gravity="center"
                           android:textColor="@color/blue"
                           android:layout_height="wrap_content"/>
                   </LinearLayout>
               </androidx.cardview.widget.CardView>
            <androidx.cardview.widget.CardView
                android:layout_width="@dimen/_150sdp"
                   app:cardBackgroundColor="@color/Aztec_Purple"
                android:id="@+id/caed_language_four"
                android:layout_marginStart="@dimen/_4sdp"
                app:cardCornerRadius="@dimen/_10sdp"
                android:layout_height="match_parent">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:orientation="vertical"
                    android:layout_margin="@dimen/_8sdp"
                    android:layout_height="match_parent">
                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_weight="1"
                        android:padding="@dimen/_12sdp"
                        android:src="@drawable/a17"
                        android:layout_height="wrap_content"/>
                    <TextView
                        android:layout_width="match_parent"
                        android:text="hwllo"
                        android:textSize="@dimen/_14ssp"
                        android:gravity="center"
                        android:textColor="@color/blue"
                        android:layout_height="wrap_content"/>
                </LinearLayout>
            </androidx.cardview.widget.CardView>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:orientation="horizontal"
            android:layout_marginTop="@dimen/_16sdp"
            android:layout_marginHorizontal="@dimen/_4sdp"
            android:layout_height="@dimen/_100sdp">
               <androidx.cardview.widget.CardView
                   android:layout_width="@dimen/_150sdp"
                   app:cardBackgroundColor="@color/Bashful_Pink"
                   android:id="@+id/caed_language_five"
                   android:layout_marginRight="@dimen/_4sdp"
                    app:cardCornerRadius="@dimen/_10sdp"
                   android:layout_height="match_parent">
                   <LinearLayout
                       android:layout_width="match_parent"
                       android:orientation="vertical"
                       android:layout_margin="@dimen/_8sdp"
                       android:layout_height="match_parent">
                        <ImageView
                            android:layout_width="match_parent"
                            android:layout_weight="1"
                            android:src="@drawable/a17"
                            android:padding="@dimen/_12sdp"
                            android:layout_height="wrap_content"/>
                       <TextView
                           android:layout_width="match_parent"
                           android:text="hwllo"

                           android:textSize="@dimen/_14ssp"
                           android:gravity="center"
                           android:textColor="@color/blue"
                           android:layout_height="wrap_content"/>
                   </LinearLayout>
               </androidx.cardview.widget.CardView>
            <androidx.cardview.widget.CardView
                android:layout_width="@dimen/_150sdp"
                   app:cardBackgroundColor="@color/Bee_Yellow"
                android:id="@+id/caed_language_six"
                android:layout_marginStart="@dimen/_4sdp"
                app:cardCornerRadius="@dimen/_10sdp"
                android:layout_height="match_parent">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:orientation="vertical"
                    android:layout_margin="@dimen/_8sdp"
                    android:layout_height="match_parent">
                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_weight="1"
                        android:src="@drawable/a8"
                        android:padding="@dimen/_12sdp"
                        android:layout_height="wrap_content"/>
                    <TextView
                        android:layout_width="match_parent"
                        android:text="hwllo"
                        android:textSize="@dimen/_14ssp"
                        android:gravity="center"
                        android:textColor="@color/blue"
                        android:layout_height="wrap_content"/>
                </LinearLayout>
            </androidx.cardview.widget.CardView>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:orientation="horizontal"
            android:gravity="center"
            android:layout_marginTop="@dimen/_16sdp"
            android:layout_marginHorizontal="@dimen/_4sdp"
            android:layout_height="@dimen/_100sdp">
               <androidx.cardview.widget.CardView
                   android:id="@+id/caed_language_seven"
                   android:layout_width="@dimen/_150sdp"
                   app:cardBackgroundColor="@color/Bright_Neon_Pink"
                  android:layout_gravity="center"
                   android:layout_marginRight="@dimen/_4sdp"
                    app:cardCornerRadius="@dimen/_10sdp"
                   android:layout_height="match_parent">
                   <LinearLayout
                       android:layout_width="match_parent"
                       android:orientation="vertical"
                       android:layout_margin="@dimen/_8sdp"
                       android:layout_height="match_parent">
                        <ImageView
                            android:layout_width="match_parent"
                            android:layout_weight="1"
                            android:padding="@dimen/_12sdp"
                            android:src="@drawable/a12"
                            android:layout_height="wrap_content"/>
                       <TextView
                           android:layout_width="match_parent"
                           android:text="hwllo"
                           android:textSize="@dimen/_14ssp"
                           android:gravity="center"
                           android:textColor="@color/blue"
                           android:layout_height="wrap_content"/>
                   </LinearLayout>
               </androidx.cardview.widget.CardView>

        </LinearLayout>

    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>